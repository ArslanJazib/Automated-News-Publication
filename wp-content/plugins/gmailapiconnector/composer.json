{
    "name": "me/gmailapiconnector",
    "description": "Plugin to extract and manage content using Gmail API",
    "type": "project",
    "license": "GPL",
    "minimum-stability": "dev",
    "require": {
        "google/apiclient": "2.12.1",
        "smalot/pdfparser": "^2.5"
